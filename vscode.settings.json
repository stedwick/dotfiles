// "$HOME/Library/Application Support/Code/User/settings.json"
{
    "editor.fontSize": 15,
    "files.autoSave": "onFocusChange",
    "files.associations": {
        "bashrc": "shellscript",
        "*.conf*": "shellscript",
        "liquidpromptrc": "shellscript",
        "vimrc*": "viml"
    },
    "editor.wordWrap": "on",
    "git.autofetch": true,
    "git.confirmSync": false,
}
