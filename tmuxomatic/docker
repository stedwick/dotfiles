0 run exec watch --interval 10 --no-title 'docker ps'
A run exec watch -n 10 -t 'docker ps --all --format "table {{.Names}}\t{{.Status}}"'
1 run exec watch -n 10 -t 'docker image ls --format "table {{.Repository}}"'
B run exec watch -n 10 -t 'docker volume ls --format "table {{.Name}}"'
C run exec watch -n 10 -t 'docker network ls --format "table {{.Name}}"'
D run exec watch -n 10 -t 'tmux ls | cut -d " " -f 1,2,3 | column -t'
